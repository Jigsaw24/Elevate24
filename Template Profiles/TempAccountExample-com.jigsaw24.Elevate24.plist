<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <!--Branding Icon and Text -->
    <key>heading</key>
    <string>Elevate24</string>
    <key>message</key>
    <string>Use of admin accounts is audited. \nPlease select why you require \nadmin rights?</string>
    <key>mainbutton</key>
    <string>Elevate Me</string>
    <key>HideLogo</key>
    <false/>
    <!--Elevated Session Customisation -->
    <key>Sessiontime</key>
    <string>1800</string>
    <key>Enabletimelist</key>
    <false/>
    <key>reasons</key>
    <array>
        <string>Install Application</string>
        <string>Update Application</string>
        <string>Uninstall Application</string>
        <string>Customise System Settings</string>
    </array>
    <key>enableReason</key>
    <false/>
       <!--Security Settings -->
    <key>standardatLoad</key>
    <true/>
    <key>enableAppleAuth</key> 
    <true/>
    <key>demoteAllAdmin</key>  
    <true/>
    <key>demoteExclusions</key> 
    <array> 
        <string>AdminTest</string> 
        <string>JamfAdmin</string> 
    </array>
    <key>userElevateAdmin</key> 
    <true/>
    <key>useCurrentUserAsAdmin</key>
    <true/>
    <key>demoteAllAdminQuit</key>
    <true/>
    <key>killterminalsessions</key>
    <true/>
    <key>showAdminPasswordGrace</key>
    <integer>30</integer>
  </dict>
</plist>